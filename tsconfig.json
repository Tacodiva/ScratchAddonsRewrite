{
    "compilerOptions": {
        "rootDir": ".",
        "outDir": "static",
        "target": "es2022",
        "module": "es2022",
        "allowJs": true,
        "strict": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitOverride": true,
        "skipLibCheck": true,
        "moduleResolution": "node",
    }
}